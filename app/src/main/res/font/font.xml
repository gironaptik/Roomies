<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- bold -->
    <font
        android:font="@font/proximabold"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/proximabold"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- regular -->
    <font
        android:font="@font/proximaregular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/proximaregular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- thin -->
    <font
        android:font="@font/proximathin"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/proximathin"
        app:fontStyle="normal"
        app:fontWeight="400" />

</font-family>